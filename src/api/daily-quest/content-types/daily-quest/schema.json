{
  "kind": "collectionType",
  "collectionName": "daily_quests",
  "info": {
    "singularName": "daily-quest",
    "pluralName": "daily-quests",
    "displayName": "DailyQuest",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "daily_quest_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::daily-quest-progress.daily-quest-progress",
      "mappedBy": "daily_quest"
    },
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "total_progress": {
      "type": "integer",
      "default": 1,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "qid": {
      "type": "uid"
    },
    "rewards": {
      "type": "component",
      "repeatable": true,
      "component": "reward.with-amount",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "days": {
      "type": "string",
      "regex": "^(Sun|Mon|Tue|Wed|Thu|Fri|Sat)(,(Sun|Mon|Tue|Wed|Thu|Fri|Sat))*$",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
