{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "desc": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "item_code": {
      "type": "integer",
      "default": -1,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "category": {
      "type": "enumeration",
      "enum": ["decoration", "built-in"],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "rarity": {
      "type": "enumeration",
      "enum": ["none", "common", "uncommon", "rare", "unique", "secret"],
      "default": "none",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "type": "integer",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "inventories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory.inventory",
      "mappedBy": "item"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "current_serial_number": {
      "type": "integer",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "room": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::room.room",
      "inversedBy": "items"
    },
    "attribute": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "additional_images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "character": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::character.character",
      "inversedBy": "items"
    }
  }
}
