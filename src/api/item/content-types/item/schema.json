{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "desc": {
      "type": "text"
    },
    "item_code": {
      "type": "integer",
      "default": -1,
      "unique": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "decoration",
        "expendable"
      ]
    },
    "rarity": {
      "type": "enumeration",
      "enum": [
        "common",
        "uncommon",
        "rare",
        "super rare",
        "unique"
      ]
    },
    "price": {
      "type": "integer"
    },
    "inventory": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::inventory.inventory",
      "inversedBy": "item"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "current_serial_number": {
      "type": "integer"
    }
  }
}
