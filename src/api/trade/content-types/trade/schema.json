{
  "kind": "collectionType",
  "collectionName": "trades",
  "info": {
    "singularName": "trade",
    "pluralName": "trades",
    "displayName": "Trade",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "expires": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "proposed",
        "counter_proposed",
        "success",
        "failed",
        "canceled",
        "rejected",
        "expired"
      ]
    },
    "history": {
      "displayName": "trade_history",
      "type": "component",
      "repeatable": true,
      "component": "trade-history.trade-history"
    },
    "proposer_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory.inventory"
    },
    "partner_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory.inventory"
    },
    "proposer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "partner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "proposer_read": {
      "type": "boolean"
    },
    "partner_read": {
      "type": "boolean"
    }
  }
}
