{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "freebie": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::freebie.freebie",
      "mappedBy": "users_permissions_user"
    },
    "level": {
      "type": "integer",
      "default": 1
    },
    "experience": {
      "type": "integer"
    },
    "quest_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quest-progress.quest-progress",
      "mappedBy": "users_permissions_user"
    },
    "rooms": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::room.room",
      "inversedBy": "users"
    },
    "inventory": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory.inventory",
      "mappedBy": "users_permissions_user"
    },
    "daily_quest_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::daily-quest-progress.daily-quest-progress",
      "mappedBy": "users_permissions_user"
    },
    "streak": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::streak.streak",
      "mappedBy": "users_permissions_user"
    },
    "followings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::creator.creator",
      "inversedBy": "followers"
    },
    "age_range": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "user_rooms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-room.user-room",
      "mappedBy": "user"
    },
    "activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity.activity",
      "mappedBy": "user"
    },
    "star_point": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::star-point.star-point",
      "mappedBy": "user"
    },
    "achievement_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::achievement-progress.achievement-progress",
      "mappedBy": "user"
    },
    "deco_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::deco-item.deco-item",
      "mappedBy": "uploader"
    }
  }
}
